{
    "ConnectionStrings": {
      "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=ReportBuilder_Test;Trusted_Connection=True;MultipleActiveResultSets=true;TrustServerCertificate=True",
      "InMemoryDatabase": "InMemoryTestDb"
    },
    
    "Logging": {
      "LogLevel": {
        "Default": "Warning",
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "Microsoft.EntityFrameworkCore": "Warning",
        "JEGASolutions": "Information"
      }
    },
  
    "AllowedHosts": "*",
  
    "JwtSettings": {
      "SecretKey": "TEST-SECRET-KEY-FOR-UNIT-TESTING-MINIMUM-32-CHARACTERS-LONG",
      "Issuer": "JEGASolutions.Test",
      "Audience": "JEGASolutions.Test.Users",
      "ExpirationMinutes": 60,
      "RefreshTokenExpirationDays": 7
    },
  
    "TestSettings": {
      "UseInMemoryDatabase": true,
      "SeedTestData": true,
      "EnableDetailedErrors": true,
      "EnableSensitiveDataLogging": true
    },
  
    "MultiTenancy": {
      "Enabled": true,
      "DefaultTenantId": 1,
      "TestTenants": [
        {
          "TenantId": 1,
          "Name": "Test Tenant 1",
          "DatabaseName": "ReportBuilder_Tenant1_Test"
        },
        {
          "TenantId": 2,
          "Name": "Test Tenant 2",
          "DatabaseName": "ReportBuilder_Tenant2_Test"
        }
      ]
    },
  
    "FileUpload": {
      "MaxFileSizeBytes": 10485760,
      "AllowedExtensions": [".xlsx", ".xls"],
      "UploadPath": "./TestUploads",
      "TempPath": "./TestTemp"
    },
  
    "ExcelProcessing": {
      "MaxRowsToProcess": 10000,
      "MaxColumnsToProcess": 100,
      "EnableValidation": true,
      "ProcessingTimeout": 30000
    },
  
    "AIServices": {
      "Provider": "Mock",
      "OpenAI": {
        "ApiKey": "test-openai-api-key",
        "Model": "gpt-4",
        "MaxTokens": 2000,
        "Temperature": 0.7,
        "Enabled": false
      },
      "Gemini": {
        "ApiKey": "test-gemini-api-key",
        "Model": "gemini-pro",
        "Enabled": false
      },
      "Claude": {
        "ApiKey": "test-claude-api-key",
        "Model": "claude-3-sonnet",
        "Enabled": false
      },
      "Mock": {
        "Enabled": true,
        "ResponseDelay": 100,
        "AlwaysSucceed": true
      }
    },
  
    "Cors": {
      "AllowedOrigins": [
        "http://localhost:3000",
        "http://localhost:5173",
        "http://localhost:4200"
      ],
      "AllowCredentials": true
    },
  
    "Email": {
      "Provider": "Mock",
      "SmtpServer": "smtp.test.com",
      "SmtpPort": 587,
      "Username": "test@jegasolutions.com",
      "Password": "test-password",
      "FromEmail": "noreply@jegasolutions.com",
      "FromName": "JEGASolutions Test",
      "EnableSsl": true,
      "Enabled": false
    },
  
    "Cache": {
      "Provider": "InMemory",
      "Redis": {
        "ConnectionString": "localhost:6379",
        "InstanceName": "ReportBuilder_Test:",
        "Enabled": false
      },
      "DefaultExpirationMinutes": 10
    },
  
    "FeatureFlags": {
      "EnableConsolidatedTemplates": true,
      "EnableHybridBuilder": true,
      "EnableExcelUploads": true,
      "EnableAIAnalysis": false,
      "EnableAdvancedReporting": true,
      "EnableRealTimeCollaboration": false
    },
  
    "Security": {
      "EnableRateLimiting": false,
      "EnableIpWhitelist": false,
      "RequireHttps": false,
      "MaxLoginAttempts": 5,
      "LockoutDurationMinutes": 15
    },
  
    "Performance": {
      "EnableResponseCaching": false,
      "EnableQueryOptimization": true,
      "MaxConcurrentRequests": 100,
      "RequestTimeoutSeconds": 30
    },
  
